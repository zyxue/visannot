{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": [2, 2],
    "comma-dangle": ["error", "never"],
    "no-plusplus": 0,
    "func-names": 0,
    "arrow-body-style": ["error", "as-needed"],
    "no-param-reassign": 0,
    "space-before-function-paren": 0,
    "react/forbid-prop-types": 0,
    "react/no-find-dom-node": 0,
    "react/no-multi-comp": 0
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}
